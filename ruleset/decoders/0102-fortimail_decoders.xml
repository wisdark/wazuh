<decoder name="fortimail-like">
   <prematch>time=\S+ device_id=\S+ log_id=\S+ |time=\S+ devid=\S+ log_id=\S+</prematch>
</decoder>

<decoder name="fortimail-like-child">
   <parent>fortimail-like</parent>
   <regex>pri=(\w+)</regex>
